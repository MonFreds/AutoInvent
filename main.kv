        FloatLayout:
            AsyncImage:
                source:'imagenes/pizap.com15234302397901.jpg'
                size_hint: 1.1,1
                pos_hint: {'center_x':.5, 'center_y': .5}

            Button:
                text: 'MERCANCIA'
                size_hint: .3, .3
                pos_hint: {'center_x':.19, 'center_y': .4}
                background_normal: 'imagenes/mercancia.png'
                on_press:app.root.camaraAccion()

            Button:
                id:tb_proveedores
                text: 'PROVEEDORES'
                size_hint: .25, .3
                pos_hint: {'center_x':.27, 'center_y': .75}
                background_normal: 'imagenes/proveedor.png'
                on_press:app.root.opciones_proveedores()

            Button:
                text: 'VENTAS'
                size_hint: .22, .3
                pos_hint: {'center_x':.7, 'center_y': .75}
                background_normal: 'imagenes/ventas.png'
                on_press:app.root.opciones_ventas()

            Button:
                text: 'USUARIOS'
                size_hint: .2, .28
                pos_hint: {'center_x':.77, 'center_y': .4}
                background_normal: 'imagenes/usuarios.png'
                on_press:app.root.opciones_usuarios()

            Button:
                id: bt_salir
                size_hint: .06, .07
                pos_hint: {'center_x':.94, 'center_y': .07}
                background_normal: 'imagenes/salir.png'
                on_press:  app.root.login()

